<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap"
        rel="stylesheet">

    <title>Cyborg - Awesome HTML5 Template</title>

    <!-- Bootstrap core CSS -->
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">


    <!-- Additional CSS Files -->
    <link rel="stylesheet" href="stylesheets/fontawesome.css">
    <link rel="stylesheet" href="stylesheets/templatemo-cyborg-gaming.css">
    <link rel="stylesheet" href="stylesheets/owl.css">
    <link rel="stylesheet" href="stylesheets/animate.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css" />
    <!--

TemplateMo 579 Cyborg Gaming

https://templatemo.com/tm-579-cyborg-gaming

-->
</head>

<body>

    <!-- ***** Preloader Start ***** -->
    <div id="js-preloader" class="js-preloader">
        <div class="preloader-inner">
            <span class="dot"></span>
            <div class="dots">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- ***** Preloader End ***** -->

    <!-- ***** Header Area Start ***** -->
    <header class="header-area header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Start *****-->
                        <a href="/" class="logo">
                            <img src="images/logo.png" alt="">
                        </a>
                        <!-- ***** Logo End ***** -->
                       
                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li><a href="/" class="active">Home</a></li>
                            <li><a href="browse">Browse</a></li>
                            <li><a href="streams">Streams</a></li>
                            <li><a href="profile">Profile <img src="images/profile-header.jpg" alt=""></a>
                            </li>
                        </ul>
                        <a class='menu-trigger'>
                            <span>Menu</span>
                        </a>
                        <!-- ***** Menu End ***** -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ***** Header Area End ***** -->

    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="page-content">

                    <!-- ***** Banner Start ***** -->
                    <div class="main-banner">
                        <div class="row">
                            <div class="col-lg-7">
                                <div class="header-text">
                                    <h6>Welcome To Up Skill</h6>
                                    <h4><em>Browse</em> Our Popular Course Here</h4>
                                    <div class="main-button">
                                        <a href="browse">Browse Now</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ***** Banner End ***** -->

                    <!-- ***** Most Popular Start ***** -->
                    <div class="most-popular">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="heading-section">
                                    <h4><em>Most Popular</em> Category</h4>
                                </div>
                                <div class="row">
                                    <% for(let i = 0; i< popularCategories.length; i++){ %>
                                        <div class="col-lg-3 col-sm-6">
                                            <div class="item">
                                                <img src= <%= popularCategories[i].image %> alt="">
                                                <h4><%= popularCategories[i].name %><br><span><%= popularCategories[i].type %></span></h4>
                                                <ul>
                                                    <li><i class="fa fa-star"></i> <%= popularCategories[i].rating %></li>
                                                    <li><i class="fa fa-graduation-cap"></i> <%= popularCategories[i].totalLearner %></li>
                                                </ul>
                                            </div>
                                        </div>
                                    <% } %>
                                    
                                    <div class="col-lg-12">
                                        <div class="main-button">
                                            <a href="browse">Discover Popular</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ***** Most Popular End ***** -->

                    <!-- ***** Most Popular Start ***** -->
                    <div class="most-popular">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="heading-section">
                                    <h4><em>Most Popular</em> Course</h4>
                                </div>
                                <div class="row">
                                    <% for(let i = 0; i < popularCourses.length; i++) { %>
                                        <div class="col-lg-4 col-sm-6"  onclick="courseDetail('<%= popularCourses[i].id %>')">
                                            <div class="item">
                                                <img src= <%= popularCourses[i].image %> alt="">
                                                
                                                <h4><%= popularCourses[i].title.substring(0, 20) + " ..." %><br><span><%= popularCourses[i].educator %></span></h4>
                                                <ul>
                                                    <li><i class="fa fa-star"></i> <%= popularCourses[i].rating %></li>
                                                    <% if(popularCourses[i].type == "Videos") { %>
                                                        <li><i class="fa fa-video"></i> Videos</li>
                                                    <% } else { %>
                                                        <li><i class="fa fa-newspaper"></i> Article</li>
                                                    <% } %>
                                                </ul>
                                            </div>
                                        </div>
                                    <% } %>


                                                                       
                                    <div class="col-lg-12">
                                        <div class="main-button">
                                            <a href="browse">Discover Popular</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ***** Most Popular End ***** -->

                    <!-- ***** Gaming Library Start ***** -->
                    <div class="gaming-library">
                        <div class="col-lg-12">
                            <div class="heading-section">
                                <h4><em>Your Course</em> Library</h4>
                            </div>
                            <% for(let i = 0; i < userLibraries.length; i++) {%>
                                <div class="item">
                                
                                    <ul>
                                        <li><img src=<%= userLibraries[i].image %> alt="" class="templatemo-item"></li>
                                        <li>
                                            <h4><%= userLibraries[i].title %></h4><span>Videos</span>
                                        </li>
                                        <li>
                                            <h4>Start Learn</h4><span><%= userLibraries[i].start %></span>
                                        </li>
                                        <li>
                                            <h4>Duration Left</h4><span><%= userLibraries[i].durationLeft %></span>
                                        </li>
                                        <li>
                                            <h4>Current Status</h4><span><%= userLibraries[i].status %></span>
                                        </li>
                                        <li>
                                            <% if(userLibraries[i].status == "Completed"){ %>
                                                <div class="main-border-button border-no-active">
                                                    <a href="#">Certificate</a>
                                                </div>
                                                
                                           <% } else if(userLibraries[i].status == "Progress"){ %>
                                                <div class="main-border-button"><a href="#">Continue</a></div>
                                            <% } else { %>
                                                <div class="main-border-button border-no-active">
                                                    <a href="#">Try Again</a>
                                                </div>
                                            <% } %>
                                        </li>
                                    </ul>
                                </div>
                            <%}%>
                            
                           
                            
                        </div>
                        <div class="col-lg-12">
                            <div class="main-button">
                                <a href="profile">View Your Library</a>
                            </div>
                        </div>
                    </div>
                    <!-- ***** Gaming Library End ***** -->
                </div>
            </div>
        </div>
    </div>

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright © 2022 <a href="#">Dewi Susanti</a>. All rights reserved.

                        
                    </p>
                </div>
            </div>
        </div>
    </footer>


    <!-- Scripts -->
    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>

    <script src="javascripts/isotope.min.js"></script>
    <script src="javascripts/owl-carousel.js"></script>
    <script src="javascripts/tabs.js"></script>
    <script src="javascripts/popup.js"></script>
    <script src="javascripts/custom.js"></script>


</body>

</html>